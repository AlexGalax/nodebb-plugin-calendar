<div class="event panel panel-default" data-id="{id}">
  <div class="panel-heading">{name}</div>
  <div class="panel-body">
    <p class="time">
      <span class="date-timestamp"
        data-timestamp="{start}"
        data-allday="{allday}"
        data-onlytime="{onlytime}"></span> -
      <span class="date-timestamp"
        data-timestamp="{end}"
        data-allday="{allday}"
        data-onlytime="{onlytime}"></span>
    </p>
    <p class="place">
      {place}
    </p>
  </div>
</div>
